import numpy as np
#array operations

#adiitions

a = np.array([1, 2, 3])
b = np.array([4, 5, 6])
print(a+b)
#multiplcation
print (a * 2)

#broadcasting
matrix = np.array([[1, 2], [3, 4], [5, 6]])
vector = np.array([0.5, 2.5])

print (matrix * vector)

#universal functions (ufuncs)
print(np.sqrt(a))
print(np.exp(a))